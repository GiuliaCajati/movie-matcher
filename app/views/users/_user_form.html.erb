
<%= form_for @user do |f| %>
<%= f.label :name, "User Name:" %>
<%= f.text_field :name %>
<br>
<br>
<br>
<%= f.label :age, "Age:" %>
<%= f.number_field :age %>
<br>
<br>
<br>
<%= f.label :phone_number, "Phone Number:" %>
<%= f.text_field :phone_number %>
<br>
<br>
<br>
<%= f.label :bio, "Bio:" %>
<%= f.text_field :bio %>
<br>
<br>
<br>
<%= f.label :photo, "Picture:" %>
<%= f.text_field :photo %>
<br>
<br>
<br>
<%= f.label :city_id, "City:" %>
<%= f.collection_select(:city_id, City.all, :id, :name)%>
<br>
<br>
<%= f.submit button_text %>
<% end %>

